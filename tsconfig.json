{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client"]
  },
 "include": [
  "src/**/*.ts",
  "src/**/*.vue",
  "env.d.ts",
  "vite.config.ts",
  "vitest.config.ts"
]

}
